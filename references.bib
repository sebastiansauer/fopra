	note = {Bandiera\_abtest: a
Cc\_license\_type: cc\_by
Cg\_type: Nature Research Journals
Number: 7876
Primary\_atype: Research
Publisher: Nature Publishing Group
Subject\_term: Decision making;Human behaviour
Subject\_term\_id: decision-making;human-behaviour},
	keywords = {Decision making, Human behaviour},
	file = {Dai et al_(2021)Behavioural nudges increase COVID-19 vaccinations.pdf:/Users/sebastiansaueruser/Zotero/storage/ZC9S9J52/Dai et al_(2021)Behavioural nudges increase COVID-19 vaccinations.pdf:application/pdf;Snapshot:/Users/sebastiansaueruser/Zotero/storage/JRTXNB6N/s41586-021-03843-2.html:text/html}
}


